{
  "name": "frontend",
  "version": "0.2.0",
  "private": true,
  "homepage": "/",
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-optional-chaining": "^7.24.8",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.11",
    "@mui/lab": "^5.0.0-alpha.166",
    "@mui/material": "^5.15.11",
    "@mui/x-data-grid": "^6.19.5",
    "@mui/x-date-pickers": "^6.19.5",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "axios": "^1.6.7",
    "browserslist": "^4.23.3",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "eslint-plugin-import": "^2.30.0",
    "i18next": "^23.10.0",
    "i18next-http-backend": "^2.6.1",
    "javascript-time-ago": "^2.5.9",
    "next": "^14.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.0.5",
    "react-router-dom": "^6.26.2",
    "react-scripts": "5.0.1",
    "react-snap": "^1.23.0",
    "sitemap": "^8.0.0",
    "use-debounce": "^10.0.0",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/jest": "^29.5.12",
    "@types/next": "^9.0.0",
    "@types/node": "^22.5.4",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.35.2",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0"
  },
  "overrides": {
    "react-scripts": {
      "typescript": "^5.5.4"
    }
  },
  "scripts": {
    "update-browserslist": "npx update-browserslist-db@latest",
    "build-sitemap": "tsc sitemap-generator.ts && node sitemap-generator.js",
    "build-robots.txt": "tsc robots-generator.ts && node robots-generator.js",
    "build-extras": "npm run build-sitemap && npm run build-robots.txt",
    "depcheck": "node depcheck.js",
    "start": "react-scripts start",
    "dev": "WATCHPACK_POLLING=true react-scripts start",
    "build": "react-scripts build",
    "postbuild": "react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "reactSnap": {
    "inlineCss": true,
    "removeBlobs": true,
    "skipThirdPartyRequests": true,
    "minifyHtml": {
      "collapseWhitespace": true
    },
    "crawl": false,
    "include": [
      "/",
      "/privacy-policy",
      "/terms-of-service"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
